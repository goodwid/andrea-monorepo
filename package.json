  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "init": "./acl-script/init",
    "lint:packages": "eslint packages/**/src",
    "lint:configs": "eslint *.js",
    "lint": "yarn run lint:packages && yarn run lint:configs",
    "lint:watch": "esw -w --clear packages/*/src",
    "start:admin": "webpack-dev-server --env.app=admin --hot --mode=development",
    "build:admin": "webpack --env.app=admin --mode=production",
    "build:admin:dev": "webpack --env.app=admin --progress --mode=development",
    "start:gen": "webpack-dev-server --env.app=gen --hot --mode=development",
    "build:gen": "webpack --env.app=gen --mode=production",
    "build:gen:dev": "webpack --env.app=gen --progress --mode=development",
    "start:rosselle": "webpack-dev-server --env.app=rosselle --hot --mode=development",
    "build:rosselle": "webpack --env.app=rosselle --mode=production",
    "build:rosselle:dev": "webpack --env.app=rosselle --progress --mode=development",
    "start:oneroom": "webpack-dev-server --env.app=oneroom --hot --mode=development",
    "build:oneroom": "webpack --env.app=oneroom --mode=production",
    "build:oneroom:dev": "webpack --env.app=oneroom --progress --mode=development",
    "test": "yarn workspaces run test -i",
    "test:watch": "yarn run test --watch",
    "deploy": "yarn run build:gen && firebase deploy",
    "deploy:database": "firebase-bolt rtdb.rules.bolt && firebase deploy --only database"
  },
  "devDependencies": {
