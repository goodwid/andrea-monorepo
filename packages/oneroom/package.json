{
  "name": "oneroom",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "pretest": "yarn run lint",
    "build": "cd ../.. && yarn build:oneroom",
    "deploy": "yarn run build && yarn run clean:remote && yarn run upload",
    "upload": "scp ./build/* devops@oneroomarthouse.com:/httpdocs/",
    "clean:remote": "ssh devops@oneroomarthouse.com \"rm -r /httpdocs/*\"",
    "start": "cd ../.. && yarn start:oneroom",
    "test": "yarn run jest",
    "jest": "jest",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "rss-converter": "^1.1.6",
    "rss-parser": "^3.9.0"
  }
}
