{
  "name": "oneroom",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "pretest": "yarn run lint",
    "build": "cd ../.. && yarn build:oneroom",
    "deploy": "yarn run build && yarn run clean:remote && yarn run upload",
    "upload": "scp ./build/* devops@oneroomarthouse.com:/httpdocs/",
    "clean:remote": "ssh devops@oneroomarthouse.com \"rm -r /httpdocs/*\"",
    "start": "cd ../.. && yarn start:oneroom",
    "test": "yarn run jest",
    "jest": "jest",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "chance": "^1.1.6",
    "firebase": "^7.18.0",
    "ftp-deploy": "^2.3.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-responsive": "^8.1.0",
    "rss-converter": "^1.1.6",
    "rss-parser": "^3.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.2.1",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.4.0",
    "sass": "^1.26.10",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.0"
  }
}
